@model ActorViewModel



<div class="form-group">
    <label class="col-form-label" asp-for="ProfilePictureURL">Profile Picture</label>
    <input class="form-control" asp-for="ProfilePictureURL" readonly />
    <span asp-validation-for="ProfilePictureURL" class="text-danegr"></span>
</div>
<div class="form-group">
    <label class="col-form-label" asp-for="FullName">Full Name</label>
    <input class="form-control" asp-for="FullName" readonly />
    <span asp-validation-for="FullName" class="text-danger"></span>
</div>
<div class="form-group">
    <label class="col-form-label" asp-for="Bio">Biography</label>
    <input class="form-control" asp-for="Bio" readonly/>
    <span asp-validation-for="Bio" class="text-danger"></span>
</div>
