@model CinemaViewModel



<div class="form-group">
    <label class="col-form-label" asp-for="LogoFile">Logo</label>
    <input class="form-control" asp-for="LogoFile" />
</div>
<div class="form-group">
    <label class="col-form-label" asp-for="FullName">Full Name</label>
    <input class="form-control" asp-for="FullName" />
    <span asp-validation-for="FullName" class="text-danger"></span>
</div>
<div class="form-group">
    <label class="col-form-label" asp-for="Description">Description</label>
    <input class="form-control" asp-for="Description" />
    <span asp-validation-for="Description" class="text-danger"></span>
</div>
<br/>